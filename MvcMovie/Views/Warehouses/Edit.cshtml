@model MvcMovie.Models.Warehouse

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>Warehouse</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="w_code" />

            <div class="form-group">
                <label asp-for="w_code" class="control-label"></label>
                <input asp-for="w_code" class="form-control" />
                <span asp-validation-for="w_code" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="w_name" class="control-label"></label>
                <input asp-for="w_name" class="form-control" />
                <span asp-validation-for="w_name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="w_amount" class="control-label"></label>
                <input asp-for="w_amount" class="form-control" />
                <span asp-validation-for="w_amount" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="w_location" class="control-label"></label>
                <input asp-for="w_location" class="form-control" />
                <span asp-validation-for="w_location" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="저장" class="btn btn-primary" />
                <a class="btn btn-warning" asp-controller="Products" asp-action="Index">취소</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
